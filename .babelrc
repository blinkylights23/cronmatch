{
  "presets": [
    "es2015",
    "stage-2",
    [
      "env",
      {
        "targets": {
          "browsers": [ "last 2 versions", "ie >= 10" ]
        }
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "plugins": [
    "add-module-exports",
    "transform-es2015-modules-umd"
  ]
}
