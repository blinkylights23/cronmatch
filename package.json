{
  "name": "@paulsmith/cronmatch",
  "version": "0.0.0-development",
  "description": "A simple cron expression parser. Compares a given date to a cron expression to see if it matches. BYOT(imer).",
  "main": "dist/index.js",
  "author": "Paul Smith <paul@paulsmith.io> (http://paulsmith.io/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blinkylights23/cronmatch"
  },
  "keywords": [
    "cron",
    "cron-expression",
    "cron-expression-parser"
  ],
  "bugs": {
    "url": "https://github.com/blinkylights23/cronmatch/issues"
  },
  "homepage": "https://github.com/blinkylights23/cronmatch",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:min": "webpack -p --output-filename index.umd.min.js",
    "commit": "git-cz",
    "check-coverage": "cross-env NODE_ENV=test nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "cover": "cross-env NODE_ENV=test nyc npm t",
    "test": "cross-env NODE_ENV=test mocha --require babel-register --compilers js:babel-register test/index.js",
    "test:watch": "cross-env NODE_ENV=test npm t -- -w",
    "semantic-release": "cross-env NODE_ENV=production semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "cross-env": "^4.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "ghooks": "^2.0.0",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.0",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.2",
    "webpack": "^2.5.0",
    "which": "^1.2.14"
  },
  "engines": {},
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 10"
            ]
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    },
    "babel": {
      "presets": [
        "es2015",
        "stage-2",
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions",
                "ie >= 10"
              ]
            }
          }
        ]
      ]
    }
  }
}
